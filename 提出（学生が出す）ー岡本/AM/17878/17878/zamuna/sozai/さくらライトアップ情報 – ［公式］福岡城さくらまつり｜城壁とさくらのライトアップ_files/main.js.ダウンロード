$.fn.isInViewport=function(){var i=$(this).offset().top,t=i+$(this).outerHeight(),n=$(window).scrollTop(),o=n+$(window).height();return t>n&&i<o},$(document).ready(function(){var i=1,t=!1,n=$(window).innerWidth(),o=$(window).innerHeight();$(".top-head").innerHeight();n<751&&$(".nav-main").css({height:o+"px"}),o<500?($(".hero").css({height:2*o+"px"}),$(".nav-main").css({overflowY:"auto"})):$(".hero").css({height:o+"px"}),$(document).on("click","#toggle-nav",function(n){if(n.preventDefault(),!t)return t=!0,i++,setTimeout(function(){t=!1},500),i%2==0?($(this).addClass("on"),$(".nav-main.is-sp").slideDown(),$(".nav-main.is-sp").css({transform:"translateX(0%)",transition:"all ease-in-out .3s"})):($(this).removeClass("on"),$(".nav-main.is-sp").css({transform:"translateX(100%)",transition:"all ease-in-out .3s"})),!1}),setTimeout(function(){$(".hero-text-bt").css({opacity:1,transition:"all ease-in-out 2s"})},6e3);$(".h_i").size();$(".nav-main .nav-item-l li > a").click(function(i){var t=$(this).attr("href"),o=$("#"===t||""===t?"html":t),e=o.offset().top-106;return $(".nav-main .nav-item-l li > a").removeClass("is-active"),$(this).addClass("is-active"),$("html, body").animate({scrollTop:e},1e3),n<751&&$("#toggle-nav").trigger("click"),!1});var e=$(".toTop");e.hide();var s=($(window).innerHeight(),$(window).innerWidth()),a=$("#footer").innerHeight(),r="";$(window).scroll(function(){$(window).scrollTop()+$(window).height()>$(document).height()-a&&s>750?$(".toTop").css({position:"absolute",bottom:"unset",top:"-40px"}):$(".toTop").css({position:"fixed",top:"auto",bottom:"4%"}),$(this).scrollTop()>0?(e.show(),$(".header-main").find(".is-home").css({opacity:1})):(e.hide(),$(".header-main").find(".is-home").css({opacity:0}))});$(".hero").innerHeight();$("#toTop").click(function(){return $("body,html").animate({scrollTop:0},500),!1})});